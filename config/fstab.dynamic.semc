# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
#
# fstab.mount:dynamic

#<src>                                    <mnt_point>    <type>  <mnt_flags and options>                                       <fs_mgr_flags>

##### ext4 #####

#system-ext4
#/dev/block/platform/msm_sdcc.1/by-num/p12  /system      ext4    ro,barrier=1                                                  wait

#cache-ext4
#/dev/block/platform/msm_sdcc.1/by-num/p13  /cache       ext4    nosuid,nodev,noatime,barrier=1,data=ordered                   wait,check

#data-ext4
#/dev/block/platform/msm_sdcc.1/by-num/p14  /data        ext4    nosuid,nodev,noatime,barrier=1,data=ordered,noauto_da_alloc   wait,check,encryptable=footer

#sdcard0-ext4
#/devices/platform/msm_sdcc.1/mmc_host/mmc0 auto         ext4    defaults,context=u:object_r:sdcard_posix:s0                   voldmanaged=sdcard0:15,nonremovable,noemulatedsd

##### f2fs #####

#cache-f2fs
#/dev/block/platform/msm_sdcc.1/by-num/p13  /cache       f2fs    noatime,nosuid,nodev,discard,nodiratime,inline_xattr,errors=recover     wait,check

#data-f2fs
#/dev/block/platform/msm_sdcc.1/by-num/p14  /data        f2fs    noatime,nosuid,nodev,discard,nodiratime,inline_xattr,errors=recover     wait,check,encryptable=footer

#sdcard0-f2fs
#/devices/platform/msm_sdcc.1/mmc_host/mmc0 auto         f2fs    discard,inline_xattr,errors=recover,context=u:object_r:sdcard_posix:s0  voldmanaged=sdcard0:15,nonremovable,noemulatedsd

##### btrfs #####

#system-btrfs
#/dev/block/platform/msm_sdcc.1/by-num/p12  /system      btrfs   ro,noatime,nodiratime,compress=lzo,subvol=system                 wait

#cache-btrfs
#/dev/block/platform/msm_sdcc.1/by-num/p13  /cache       btrfs   noatime,nosuid,nodev,nodiratime,compress=lzo,subvol=cache        wait

#data-btrfs
#/dev/block/platform/msm_sdcc.1/by-num/p14  /data        btrfs   nosuid,nodev,noatime,nodiratime,compress=lzo,subvol=data         wait,check,encryptable=footer

#sdcard0-btrfs
#/devices/platform/msm_sdcc.1/mmc_host/mmc0 auto         btrfs   compress=lzo,subvol=sdcard,context=u:object_r:sdcard_posix:s0    voldmanaged=sdcard0:15,nonremovable,noemulatedsd

##### vfat #####

#sdcard0-vfat
#/devices/platform/msm_sdcc.1/mmc_host/mmc0 auto         vfat    defaults                                voldmanaged=sdcard0:15,nonremovable,noemulatedsd

/devices/platform/msm_hsusb_host.0          auto         vfat    defaults                                voldmanaged=usbdisk:auto

